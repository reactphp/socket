{
    "name": "react/socket",
    "description": "Library for building an evented socket server.",
    "keywords": ["socket"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "evenement/evenement": "~2.0|~1.0",
        "react/event-loop": "0.4.*|0.3.*",
        "react/stream": "0.4.*|0.3.*"
    },
    "autoload": {
        "psr-4": {
            "React\\Socket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "React\\Tests\\Socket\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    }
}
